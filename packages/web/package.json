{
  "name": "@gshop/web",
  "version": "1.0.0",
  "main": "pages/index.tsx",
  "scripts": {
    "dev": "nodemon server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .next/production-server/index.js",
    "test": "jest",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "debug": "ndb nodemon server/index.ts",
    "export": "next build && next export",
    "lint": "tslint 'pages/**/*.ts{,x}' 'server/**/*.ts' --fix",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@gshop/common": "1.0.0",
    "express": "^4.16.3",
    "fastify": "^1.12.1",
    "next": "^7.0.1",
    "next-plugin-transpile-modules": "^0.1.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "reakit": "^0.15.5",
    "reakit-theme-default": "^0.1.6",
    "sitemap": "^2.0.1",
    "styled-components": "^4.0.0-beta.9",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.5",
    "@types/next": "^7.0.0",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.8",
    "@types/styled-components": "^3.0.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-typescript": "1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.0.1",
    "babel-plugin-styled-components": "^1.8.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.1.1",
    "typescript-babel-jest": "^1.0.5"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "license": "MIT"
}
